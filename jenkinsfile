pipeline {
         agent any
         stages {
                 stage('One') {
                 steps {
                     echo 'Hi, this is ramu from TATA. Build stage'
                 }
                 }
                 stage('Two') {
                 steps {
                    input('Do you want to proceed?')
                    echo 'Test stage'
                 }
                 }
                 stage('Three') {
                 when {
                       not {
                            branch "master"
                       }
                 }
                 steps {
                       echo "Hello Deployment stage"
                 }
                 }
                 
}
}
